package OCT18B;

import java.util.Scanner;

public class CHSERVE {
	public static void main(String[] args) {
		int t,n,d;
		Scanner sc = new Scanner(System.in);
		t= sc.nextInt();
		while(t-->0){
			int p1=sc.nextInt();
			int p2=sc.nextInt();
			int k=sc.nextInt();
			
			if(( (p1+p1)/k) %k==0){
				System.out.println("CHEF");
			}else{
				System.out.println("COOK");
			}	
			
		}
	
	}
}