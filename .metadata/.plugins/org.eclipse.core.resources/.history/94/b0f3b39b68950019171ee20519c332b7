		Arrays.fill(resArr,'0');
		char []arr=s.toCharArray();
		char []brr=d.toCharArray();
		int carry=0;
		char number[]=new char[base];
		for(int i=0;i<base;i++){
			if(i<10){
				number[i]=''+i;
			}
			else{
				number[i]=()
			}
			
		}
		
		
		
		for(int i=d.length()-1;i>=0;i--){
			for(int j=s.length()-1;j>=0;j--){
								
			}
		}
		
		return res;
	}
}
