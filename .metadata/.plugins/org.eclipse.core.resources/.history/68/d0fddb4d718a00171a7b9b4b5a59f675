package codechef;

import java.util.Scanner;

public class ALATE {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int t=sc.nextInt();
		sc.nextInt();
		while(t-->0){
			String str= sc.nextLine();
			System.out.println(str);
			System.out.println(str.length());
		}
	}

}
